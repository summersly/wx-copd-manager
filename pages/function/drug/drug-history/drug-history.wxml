<!--pages/function/drug/drug-history/drug-history.wxml-->
<view class='drug-container'>
    <calendar 
    binddayClick="dayClick" 
    weeks-type="cn" 
    days-color="{{dayscolor}}" 
    cell-size="38"
    bindnextMonth="dateChange"
    bindprevMonth="dateChange"
    binddateChange="dateChange"/>
</view>
<view class="history">
    <drug-his-list drugHisData="{{drugHisData}}"/>
</view>
